sgn-08    :  ⊦ sgn(i) ≠ 0 ;
  used for : s, ls ;
  remarks : proved ;
sgn-09    :  ⊦ 0 ≤ i → sgn(i) * j = j ;
  used for : s, ls ;
  remarks : proved ;
sgn-1     :  ⊦ j < 0 → sgn(j) = ~1 ;
  used for : s, ls ;
  remarks : proved ;
sr        :  ⊦ sgn(i) ≠ 1 → sgn(i) = ~1 ;
  used for : s ;
  remarks : proved ;
