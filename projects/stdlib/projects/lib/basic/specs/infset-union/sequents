        used for : f, lf ;
         remarks : proved ;
diff-idem        :  ¥ s \ s = à ;
        used for : s, ls ;
         remarks : proved ;
diff-int         :  ¥ (s \ s0) · s0 = à ;
        used for : s, ls ;
         remarks : proved ;
diff-int-01      :  ¥ (s \ (s0 ¶ s1)) · s0 = à ;
        used for : s, ls ;
         remarks : proved ;
