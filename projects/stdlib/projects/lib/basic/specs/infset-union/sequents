;; variables    ; 

lemmas

del              : a ² s ¥ ´ s0. s = s0 ++ a ¸ µ a ² s0 ;
         remarks : proved ;
del-addeq        : ¥ 
                   a Ü b ¸ µ a ² s ¸ µ a ² s0 ­ ((s ++ a) -- b = s0 ++ a ° s -- b = s0) ;
        used for : s, ls ;
         remarks : proved ;
del-addeq-01     :  ¥ (s ++ a) -- b -- a = s -- b -- a ;
        used for : s, ls ;
         remarks : proved ;
del-addeq-02     :  ¥ a ² s ­ (s -- a) ++ a = s ;
        used for : s, ls, lc ;
         remarks : proved, used by proved unit ;
del-addeq-03     :  ¥ a ² s ¸ a Ü b ­ (s -- b) ++ a = s -- b ;
        used for : s, ls, lc ;
         remarks : proved ;
del-addeq-04     :  ¥ µ a ² s ­ (s ++ a) -- a = s ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
del-addeq-05     :  ¥ (s ++ a) -- a = s -- a ;
        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
del-addeq-06     :  ¥ (s ++ a ++ b) -- a = (s ++ b) -- a ;
        used for : s, ls ;
         remarks : proved ;
del-addin        :  ¥ µ a ² s ­ (a ² (s -- a0) ++ b ++ c ° a = b ¹ a = c) ;
        used for : s, ls ;
         remarks : proved ;
del-addin-01     :  ¥ µ a ² s ­ (a ² (s -- b) ++ c ° a = c) ;
        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
