        used for : s, ls ;
         remarks : proved, used by proved unit ;
diff-union       :  ¥ a ² s0 ­ (a ² (s \ s0) ¶ s1 ° a ² s1) ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
