        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
diff-union-02    :  ¥ µ a ² s ­ (a ² ((s ¶ s2) \ s0) ¶ s1 ° a ² (s2 \ s0) ¶ s1) ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
diff-union-03    :  ¥ (s ¶ s0) \ s1 = (s \ s1) ¶ (s0 \ s1) ;
         remarks : proved, used by proved unit ;
