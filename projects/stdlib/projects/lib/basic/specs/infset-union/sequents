        used for : s, ls ;
         remarks : proved ;
del-d-01         :  ¥ s -- a -- b -- a = s -- b -- a ;
        used for : s, ls ;
         remarks : proved ;
del-del-one      :  ¥ a Ü b ­ (à ++ a) -- b = à ++ a ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
del-e            :  ¥ µ a ² s ­ ((s ++ a) -- b = à ° s = à ¸ a = b) ;
        used for : s, ls ;
         remarks : proved ;
del-e-01         :  ¥ à -- a = à ;
        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
del-e-02         :  ¥ s -- a = à ° s = à ¹ s = à ++ a ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
del-eq           :  ¥ a ² s ¸ a ² s0 ­ (s -- a = s0 -- a ° s = s0) ;
        used for : s, ls ;
         remarks : proved ;
del-feq          :  ¥ s -- a = s0 ++ b ­ a Ü b ¸ b ² s ¸ µ a ² s0 ;
        used for : f, lf ;
         remarks : proved ;
del-feq-01       :  ¥ (s ++ a) -- b = s0 ++ c ¸ µ c ² s ­ a = c ¸ b Ü c ;
        used for : f, lf ;
         remarks : proved ;
del-feq-02       :  ¥ (s ++ a) -- b = s0 ++ c ¸ a Ü b ¸ µ a ² s0 ­ a = c ;
        used for : f, lf ;
         remarks : proved ;
del-in           :  ¥ µ a ² s ­ s -- a = s ;
        used for : s, ls, lc ;
         remarks : proved, used, used by proved unit ;
del-in-01        :  ¥ a Ü b ­ (µ a ² s -- b ° µ a ² s) ;
        used for : s, ls ;
         remarks : proved ;
del-in-02        :  ¥ µ a ² s -- a ;
        used for : s, ls ;
         remarks : proved ;
del-in-03        :  ¥ s -- a = s ° µ a ² s ;
        used for : s, ls ;
         remarks : proved, used ;
del-neq          :  ¥ s -- a Ü s0 -- a ­ s Ü s0 ;
        used for : f, lf ;
         remarks : proved ;
del-neq-01       :  ¥ a Ü b ­ (s ++ a) -- b Ü à ;
        used for : s, ls ;
         remarks : proved ;
del-neq-02       :  ¥ s -- a Ü s0 ++ a ;
        used for : s, ls ;
         remarks : proved ;
del-notin        :  ¥ s0 = s -- a ­ µ a ² s0 ;
        used for : s, ls ;
         remarks : proved ;
del-rax          :  ¥ a ² s -- b ° µ µ (a Ü b ¸ a ² s) ;
        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
del-two          : s -- a -- b = à ¥ s = à ¹ s = à ++ a ¹ s = à ++ b ¹ s = à ++ a ++ b ;
         remarks : proved ;
delete-insert    :  ¥ (s -- a) ++ a = s ++ a ;
        used for : s, ls ;
         remarks : proved, used, used by proved unit ;
delete-subset    :  ¥ (s -- a) ++ b Â s ++ b ;
        used for : s, ls ;
         remarks : proved, used by proved unit ;
