            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-diff-union-same :  ¥ (s0 ¶ s1) \ (s2 ¶ s1) = s0 \ (s2 ¶ s1) ;
            used for : s ;
             remarks : proved ;
union-difference     :  ¥ s Â s0 ­ s ¶ (s0 \ s) = s0 ;
            used for : s, ls ;
             remarks : proved, used by proved unit ;
union-inter-inter-union :  ¥ (s0 ¶ s1) · s2 = (s0 · s2) ¶ (s1 · s2) ;
             remarks : proved, used by proved unit ;
union-one            :  ¥ a Ü b ­ (a ² {b}  ¶ s ° a ² s) ;
            used for : s, ls ;
             remarks : proved ;
union-rem-add        :  ¥ a ² s0 ­ ((s0 -- a) ¶ s1) ++ a = s0 ¶ s1 ;
             remarks : proved ;
union-rem-unchdg     :  ¥ (s0 -- a) ¶ s1 = s0 ¶ s1 ° a ² s1 ¹ µ a ² s0 ;
            used for : s, ls ;
             remarks : proved ;
union-rewone         :  ¥ s ++ a = s ¶ {a}  ;
             comment :  used for interactive rewriting ;
             remarks : proved, used by proved unit ;
union-size           :  ¥ µ # (s ¶ s0) < # s ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-size-01        :  ¥ µ a ² s ­ µ # ((s ¶ s0) ++ a) ¨ # s ;
            used for : s, ls ;
             remarks : proved ;
union-size-02        :  ¥ µ a ² s ¸ a ² s0 ­ # (s ¶ s0) = # (s ¶ (s0 -- a)) + 1 ;
             remarks : proved ;
union-size-03        :  ¥ # (s0 ¶ s1) = # s0 + # s1 - # (s0 · s1) ;
             remarks : proved, used ;
union-size-insert    :  ¥ µ a ² s ¸ µ a ² s0 ­ # ((s ¶ s0) ++ a) = # (s ¶ s0) + 1 ;
            used for : s, ls ;
             remarks : proved ;
union-size-le        :  ¥ # (s0 ¶ s1) ¨ # s0 + # s1 ;
             remarks : proved, used ;
union-size-ls-in     :  ¥ a ² s0 ¸ a ² s1 ­ # (s0 ¶ s1) < # s0 + # s1 ;
             remarks : proved ;
union-size-not-less  :  ¥ µ # s0 + # s1 < # (s0 ¶ s1) ;
            used for : s, ls ;
             remarks : proved ;
union-size-not-less-01 :  ¥ # s0 ¨ m ¸ # s1 ¨ n ­ µ m + n < # (s0 ¶ s1) ;
            used for : s, ls ;
             remarks : proved, used ;
union-size-not-less-02 :  ¥ # s0 ¨ m ¸ # s1 ¨ n ­ µ m + n + 1 < # (s0 ¶ s1) ;
            used for : s, ls ;
             remarks : proved ;
union-sizeeq         :  ¥ # (s ¶ s0) = # s ° s0 Â s ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-sizele         :  ¥ # (s ¶ s0) ¨ # s ° s0 Â s ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-sizele-01      :  ¥ # ((s ¶ s0) ++ a) ¨ # s ° s0 Â s ¸ a ² s ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-sub            :  ¥ s0 Â s ­ s ¶ s0 = s ;
            used for : ls ;
             remarks : proved, used, used by proved unit ;
union-sub-01         :  ¥ s0 Â s ­ s0 ¶ s = s ;
            used for : ls ;
             remarks : proved, used ;
union-subset         :  ¥ s Â s0 ­ s ¶ s1 Â s0 ¶ s1 ;
             remarks : proved ;
union-subset-01      :  ¥ µ s ¶ s1 Â s0 ¶ s1 ­ µ s Â s0 ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
union-subset-02      :  ¥ s Â s0 ¸ s1 Â s2 ­ s ¶ s1 Â (s0 ¶ s2) ++ a ;
            used for : s, ls ;
             remarks : proved ;
union-subset-03      :  ¥ s Â s0 ¸ s1 Â s2 ¸ s3 Â s4 ­ s ¶ s1 ¶ s3 Â s0 ¶ s2 ¶ s4 ;
            used for : s, ls ;
             remarks : proved ;
union-subset-04      :  ¥ s Â s0 ¸ s1 Â s2 ¸ s3 Â s4 ­ s ¶ s1 ¶ s3 Â (s0 ¶ s2 ¶ s4) ++ a ;
            used for : s, ls ;
             remarks : proved ;
union-subset-05      :  ¥ s Â s0 ­ s ¶ s1 Â (s0 ¶ s1) ++ a ;
            used for : s, ls ;
             remarks : proved ;
unrax                :  ¥ a ² s ¶ s0 ° µ µ (a ² s ¹ a ² s0) ;
            used for : ls ;
             remarks : proved, used ;
unsub                :  ¥ s Â s ¶ s0 ;
            used for : s, ls ;
             remarks : proved, used, used by proved unit ;
unsub-union-01       :  ¥ s1 ¶ s2 Â s ° s1 Â s ¸ s2 Â s ;
            used for : s, ls ;
             remarks : proved, used by proved unit ;
unsubdel             : ¥ 
                       s Â s0 ¸ s1 Â s2 ¸ µ a ² s ¸ µ a ² s1 ­ s ¶ s1 Â (s0 -- a) ¶ (s2 -- a) ;
            used for : s, ls ;
             remarks : proved ;
unsubdel-01          :  ¥ s Â s0 ¸ µ a ² s ­ (s0 -- a) ¶ s = s0 -- a ;
            used for : s, ls ;
             remarks : proved ;
unsubins             :  ¥ s ++ a Â s ¶ s0 ° a ² s ¶ s0 ;
            used for : s, ls ;
             remarks : proved ;
unsubins-01          :  ¥ s Â (s ¶ s0) ++ a ;
            used for : s, ls ;
             remarks : proved, used by proved unit ;
unsubsub             :  ¥ s Â s0 ¸ s1 Â s2 ­ s ¶ s1 Â s0 ¶ s2 ;
            used for : s, ls ;
             remarks : proved, used by proved unit ;


;;; END

