;; variables    ; 

lemmas
equal-add-in         :  ¥ s ++ a = s0 ++ b ¸ s0 ++ b = (s1 -- c) ++ a0 ¸ b Ü a0 ­ b ² s1 -- c ;
            used for : s ;
             remarks : proved ;

;;;END

